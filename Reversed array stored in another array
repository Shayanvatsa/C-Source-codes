#include <stdio.h>
void main()
{
	int i,n,j,ar[100],revAr[100];
	printf("Enter total number of elements= ");
	scanf("%d",&n);
	printf("Enter array elements:-\n");
	for(i=0;i< n;i++)
	{
		printf("Enter element %d= ",i+1);
		scanf("%d",&ar[i]);
	}
	for(i=(n-1),j=0; i>=0; i--,j++)
	{
		revAr[j]=ar[i];
	}
	printf("\nArray elements after reversing=\n");
	for(i=0;i<n;i++)
	{
		printf("%d\n",revAr[i]);
	}
}
